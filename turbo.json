{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "prisma:push": {
      "cache": false
    },
    "prisma:generate": {
      "cache": false
    },
    "docker:up": {
      "cache": false
    },
    "docker:down": {
      "cache": false
    },
    "prisma:seed": {
      "dependsOn": ["docker:up", "prisma:push", "prisma:generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "docker:up", "prisma:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "dependsOn": ["docker:up", "prisma:generate"],
      "cache": false,
      "persistent": true
    }
  }
}
