{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "prisma:generate": { "cache": false },
    "docker:up": { "cache": false },
    "build": {
      "dependsOn": ["^build", "prisma:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "dependsOn": ["prisma:generate"],
      "cache": false,
      "persistent": true
    }
  }
}
