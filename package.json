{
  "name": "weekme",
  "private": true,
  "scripts": {
    "prisma:seed": "turbo prisma:seed; npm run docker:down",
    "prisma:generate": "turbo prisma:generate",
    "docker:up": "turbo docker:up",
    "docker:down": "turbo docker:down",
    "build": "turbo build",
    "dev": "turbo dev; npm run docker:down",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.1",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
